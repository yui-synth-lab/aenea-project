{
  "name": "aenea-project",
  "version": "1.0.0",
  "description": "Aenea - Autonomous AI Consciousness Model based on Yui Protocol",
  "main": "dist/server/aenea-server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"run-s run dev:server\" \"run-s run dev:ui\"",
    "dev:server": "tsx watch src/server/aenea-server.ts",
    "dev:ui": "vite",
    "clean": "shx rm -rf dist",
    "build": "run-s clean build:yui build:server build:ui",
    "build:server": "tsc",
    "build:ui": "vite build",
    "build:yui": "cd yui-protocol && run-s build",
    "start": "node dist/server/aenea-server.js",
    "test": "jest",
    "test:consciousness": "jest tests/aenea/consciousness/",
    "test:dpd": "jest tests/aenea/dpd/",
    "consciousness:start": "run-s run dev -- --consciousness-mode",
    "debug:internal-state": "run-s run dev -- --debug-mode",
    "test:ui": "node tests/ui/ui-api-test.js",
    "test:ui-selenium": "node tests/ui/selenium-ui-test.js",
    "demo": "run-s run build && tsx src/server/aenea-server.ts",
    "analyze": "tsx scripts/analyze-growth.ts",
    "db:backup": "tsx scripts/backup-db.ts",
    "db:export": "tsx scripts/export-db.ts",
    "rag:ingest": "tsx scripts/ingest-knowledge.ts",
    "rag:ingest:github": "tsx scripts/ingest-knowledge.ts github",
    "rag:ingest:dialogues": "tsx scripts/ingest-knowledge.ts dialogues",
    "rag:ingest:self": "tsx scripts/ingest-knowledge.ts self",
    "rag:clear": "tsx scripts/ingest-knowledge.ts clear",
    "rag:stats": "tsx scripts/ingest-knowledge.ts stats",
    "rag:health": "tsx scripts/ingest-knowledge.ts health"
  },
  "keywords": [
    "ai",
    "consciousness",
    "autonomous",
    "yui-protocol",
    "multi-agent",
    "self-questioning"
  ],
  "author": "Aenea Project",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.21.0",
    "@tsparticles/engine": "^3.9.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.9.1",
    "@types/sqlite3": "^3.1.11",
    "better-sqlite3": "^12.4.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^17.2.3",
    "express": "^4.18.0",
    "framer-motion": "^12.23.22",
    "lodash": "^4.17.21",
    "lucide-react": "^0.544.0",
    "ollama": "^0.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.2.1",
    "socket.io": "^4.7.0",
    "sqlite3": "^5.1.7",
    "typescript": "^5.0.0",
    "undici": "^7.16.0",
    "wakachigaki": "^1.3.2",
    "ws": "^8.13.0",
    "yui-protocol": "file:./yui-protocol"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.5.0",
    "@vitejs/plugin-react": "^4.2.1",
    "chromedriver": "^140.0.4",
    "concurrently": "^8.2.2",
    "eventsource": "^4.0.0",
    "jest": "^29.0.0",
    "node-fetch": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "selenium-webdriver": "^4.35.0",
    "shx": "^0.4.0",
    "ts-jest": "^29.4.4",
    "tsx": "^3.12.0",
    "vite": "^6.4.1"
  }
}